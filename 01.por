programa {
  funcao inicio() {

    inteiro num1, num2, soma

    escreva("Calculadora de Soma\n")

    escreva("Escreva um número inteiro: ")
    leia(num1)

    escreva("Escreva outro número inteiro: ")
    leia(num2)


    soma = num1 + num2

    escreva("A soma dos dois números é igual a ", soma)
    
  }
}
